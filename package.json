{
  "name": "@flexis/workbox-webpack-plugin",
  "version": "1.0.0-beta",
  "description": "Webpack plugin to generate and inject precache manifest to ServiceWorker.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/flexis-workbox-webpack-plugin"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-workbox-webpack-plugin/issues"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint --cache 'src/**/*.js'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "checkSize": "size-limit",
    "test": "npm run lint && npm run jest && npm run build && npm run checkSize",
    "buildTestBundle": "NODE_ENV=test babel-node test/buildTestBundle",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "cleanPublish": "npm run test && clean-publish"
  },
  "keywords": [
    "serviceworker",
    "webpack",
    "webpack-plugin",
    "workbox",
    "precache",
    "manifest",
    "precache-manifest"
  ],
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "service-worker-loader": "*",
    "webpack-sources": "^1.3.0",
    "workbox-build": "^3.6.3",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "^7.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-trigen": "^1.8.0",
    "browserslist-config-trigen": "^1.1.0",
    "clean-publish": "^1.1.0",
    "clean-webpack-plugin": "^1.0.1",
    "eslint": "^5.11.1",
    "eslint-config-trigen": "^3.2.1",
    "eslint-plugin-jest": "^22.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "memory-fs": "^0.4.1",
    "rollup": "^1.0.1",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-json": "^3.1.0",
    "service-worker-loader": "^3.0.0",
    "size-limit": "^0.21.1",
    "webpack": "^4.28.3",
    "workbox-sw": "^3.6.3"
  },
  "files": [
    "lib"
  ]
